=content_for :main_toolbar do
  Domain Masterdata

.row
  .col-sm-8
    #domain_masterdata_details
      - if !@domain_masterdata
        .alert.alert-warning
          = icon('exclamation-circle')
          No masterdata maintained for this domain.
      - elsif !flash.now[:error]
        = render partial: 'masterdata_details'
  .col-md-4
    #domain_masterdata_additional_infos
      = render partial: 'masterdata_additional_infos'